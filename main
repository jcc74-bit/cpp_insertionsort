#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;
int main (){
    srand (time(0));
    int array[100];
    for (int i=0; i<100; i++){
        array[i]= i+1;
    }
    cout << "\n\n";
    for (int i=0; i<100; i++){
    int randIndex = rand() % 100;
    int temp = array[i];
    array[i]= array[randIndex];
    array[randIndex]= temp;

}
    for(int i=0; i<100; i++){
        cout << array[i]<< " ";
    }
    cout << "\n\n";
    
    for (int i=1; i<100-1; i++){
        int j = i;
        while (j>0 and array[j-1]>array[j]){
            int temp= array[j];
            array [j]= array[j-1];
            array [j-1]= temp;
            j=j-1;
        
        }
    }
    
    for(int i=0; i<99; i++){
        cout << array[i]<< " ";
}
}


